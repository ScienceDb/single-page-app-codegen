          newError.path=['<%- namePlCp %>', `<%- internalId _%>:${item.<%- internalId _%>}`, 'delete'];
          newError.extensions = {graphqlResponse:{data:response.data.data, errors:response.data.errors}};
          errors.current.push(newError);
          console.log("Error: ", newError);

          showMessage(newError.message, withDetails);
          clearRequestDoDelete();
          return;
        }

        //check: <%- queryName3 %>
        let <%- queryName3 %> = response.data.data.<%- queryName3 %>;
        if(<%- queryName3 %> === null) {
          let newError = {};
          let withDetails=true;
          variant.current='error';
          newError.message = '<%- queryName3 %> ' + t('modelPanels.errors.data.e5', 'could not be completed.');
          newError.locations=[{model: '<%- name -%>', query: '<%- queryName3 %>', method: '<%- methodName2 %>', request: '<%- requestName3 %>'}];
          newError.path=['<%- namePlCp %>', `<%- internalId _%>:${item.<%- internalId _%>}` ,'delete'];
          newError.extensions = {graphqlResponse:{data:response.data.data, errors:response.data.errors}};
          errors.current.push(newError);
          console.log("Error: ", newError);

          showMessage(newError.message, withDetails);
          clearRequestDoDelete();
          return;
        }

        /**
         * Type of <%- queryName3 %> is not validated. Only not null is
         * checked above to confirm successfull operation.
         */

        //check: graphql errors
        if(response.data.errors) {
          let newError = {};
          let withDetails=true;
          variant.current='info';
          newError.message = '<%- queryName3 %> ' + t('modelPanels.errors.data.e6', 'completed with errors.');
          newError.locations=[{model: '<%- name -%>', query: '<%- queryName3 %>', method: '<%- methodName2 %>', request: '<%- requestName3 %>'}];
          newError.path=['<%- namePlCp %>', `<%- internalId _%>:${item.<%- internalId _%>}` ,'delete'];
          newError.extensions = {graphQL:{data:response.data.data, errors:response.data.errors}};
          errors.current.push(newError);
          console.log("Error: ", newError);

          showMessage(newError.message, withDetails);
        }

        //ok
        enqueueSnackbar( t('modelPanels.messages.msg4', "Record deleted successfully."), {
          variant: 'success',
          preventDuplicate: false,
          persist: false,
          anchorOrigin: {
            vertical: 'bottom',
            horizontal: 'left',
          },
        });
        reloadData();
        return;
      },
      //rejected
      (err) => {
        throw err;
      })
      //error
      .catch((err) => { //error: on <%- requestName3 %>
        if(err.isCanceled) {
          return
        } else {
          let newError = {};
          let withDetails=true;
          variant.current='error';
          newError.message = t('modelPanels.errors.request.e1', 'Error in request made to server.');
