{
  "cwd": "./test/integration_test_env",
  "env": {
    "NODE_JQ_SKIP_INSTALL_BINARY": true
  },
  "docker": "../integration_test_misc/docker-compose-test.yml",
  "services": [
    {
      "name": "gql_science_db_graphql_server1",
      "template": "https://github.com/Zendro-dev/graphql-server.git",
      "branch": "master",
      "url": "http://localhost:3000/graphql"
    },
    {
      "name": "gql_science_db_graphql_server2",
      "template": "https://github.com/Zendro-dev/graphql-server.git",
      "branch": "master",
      "url": "http://localhost:3030/graphql"
    },
    {
      "name": "spa_science_db_app_server1",
      "template": "https://github.com/Zendro-dev/single-page-app.git",
      "branch": "master",
      "url": "http://localhost:8080/graphql"
    },
    {
      "name": "spa_science_db_app_server2",
      "template": "https://github.com/Zendro-dev/single-page-app.git",
      "branch": "master",
      "url": "http://localhost:8081/graphql"
    }
  ],
  "models": [
    {
      "codegen": "https://github.com/Zendro-dev/graphql-server-model-codegen.git",
      "branch": "master",
      "options": [ "--migrations" ],
      "path": "../integration_test_models_instance1",
      "targets": [ "gql_science_db_graphql_server1"  ]
    },
    {
      "codegen": "https://github.com/Zendro-dev/graphql-server-model-codegen.git",
      "branch": "master",
      "options": [ "--migrations" ],
      "path": "../integration_test_models_instance2",
      "targets": [ "gql_science_db_graphql_server2" ]
    },
    {
      "codegen": "../..",
      "opts": [ "-P", "-D" ],
      "path": "../integration_test_models_instance1",
      "targets": [ "spa_science_db_app_server1" ]
    },
    {
      "codegen": "../..",
      "opts": [ "-P", "-D" ],
      "path": "../integration_test_models_instance2",
      "targets": [ "spa_science_db_app_server2" ]
    }
  ],
  "patches": [
    {
      "path": "../../test/patches/with_validations.js.patch",
      "target": "gql_science_db_graphql_server1/validations/with_validations.js"
    },
    {
      "path": "../../test/patches/acl_rules.js.patch",
      "target": "spa_science_db_app_server1/src/acl_rules.js"
    },
    {
      "path": "../../test/patches/migrateDbAndStartServer1.patch",
      "target": "gql_science_db_graphql_server1/migrateDbAndStartServer.sh"
    },
    {
      "path": "../../test/patches/migrateDbAndStartServer2.patch",
      "target": "gql_science_db_graphql_server2/migrateDbAndStartServer.sh"
    }
  ]
}
